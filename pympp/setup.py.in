#!/usr/bin/env python

from distutils.core import setup, Extension

source = ["${MPP_MODULE_SRCS}"]
includes = ["${NUMPY_INCLUDE_DIRS}"]

# define the extension module
mpp_module = Extension('mpp_module',
    sources = source,
    include_dirs = includes,
    )

# run the setup
setup(ext_modules=[mpp_module])
